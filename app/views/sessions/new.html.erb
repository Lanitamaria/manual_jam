<h1>Login</h1>
<%= form_for(:session, url: sessions_create_path) do |f| %>
    <div class="form-group">
        <%= f.label :email %>
        <%= f.text_field :email %>
    </div>
    <div class="form-group">
        <%= f.label :password %>
        <%= f.text_field :password %>
    </div>

    <%= f.submit "Guardar" %>
<% end %>